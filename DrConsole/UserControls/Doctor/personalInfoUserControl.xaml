<UserControl x:Class="DrConsole.UserControls.Doctor.personalInfoUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DrConsole.UserControls.Doctor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" Height="397" Margin="37709692352,50,-37709692714,0" VerticalAlignment="Top" Width="1162">
        <TextBlock x:Name="expertize" TextWrapping="Wrap" Text="{Binding ElementName=doctor, Path=Expertize}" Width="1" />
        <TextBlock x:Name="liscence" TextWrapping="Wrap" Text="{Binding ElementName=doctor, Path=Licence}" Width="1"/>
        <TextBlock x:Name="lastName" TextWrapping="Wrap" Text="{Binding ElementName=doctor, Path=LastName}" Width="1"/>
        <TextBlock x:Name="firstName" TextWrapping="Wrap" Text="{Binding ElementName=doctor, Path=FirstName}" Width="1"/>



    </StackPanel>



</UserControl>
